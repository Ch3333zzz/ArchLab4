; простая проверка присвоений, if и цикла while
    (setq x 1)
    (print (if (= 1 x) "T" "F"))   ; ожидаем "T"
    (setq x (if (= 1 x) 2 3))      ; теперь x = 2
    (setq x 0)
    (while (< x 5)
      (setq x (+ x 1))
    )
    (print (setq x 13))            ; присваивание возвращает 13 -> печатается "13"